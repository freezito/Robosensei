await movefor(100,100,1);
while (true)
{
    await readInputs();
    if ( irLeft() < 50 )
    {
        move(-40, 70);
    }     
      else if( irLeft() < 50)
    {
        move(-100, 100);
    }
    else if ( irRight() < 50)
    {
        move(100, -100);
    }
    else
    {
       move(100, 100);
    }
}
